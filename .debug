<?xml version="1.0" encoding="UTF-8"?>
<ExtensionList>
	<Extension Id="com.sharif.boilerplate.panel">
		<HostList>
			<!-- Comment Host tags according to the apps you want to support -->
			<!-- Photoshop -->
			<Host Name="PHXS" Port="8088" />
			<!-- InDesign -->
			<Host Name="IDSN" Port="8087" />
			<!-- InCopy -->
			<Host Name="AICY" Port="8086" />
			<!-- Illustrator -->
			<Host Name="ILST" Port="8085" />
			<!-- Premiere -->
			<Host Name="PPRO" Port="8084" />
			<!-- AfterEffects -->
			<Host Name="AEFT" Port="8083" />
			<!-- Prelude -->
			<Host Name="PRLD" Port="8082" />
			<!-- FLASH Pro -->
			<Host Name="FLPR" Port="8081" />
			<!-- Dreamweaver -->
			<Host Name="DRWV" Port="8080" />
			<!-- Audition -->
			<Host Name="AUDT" Port="8079" />
			<!-- Muse -->
			<Host Name="MUSE" Port="8078" />
			<!-- Bridge -->
			<Host Name="KBRG" Port="8077" />
		</HostList>
	</Extension>
</ExtensionList>
